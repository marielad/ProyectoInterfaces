<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<fx:root prefHeight="480.0" prefWidth="640.0" type="StackPane" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<Pane fx:id="paneNubes" prefHeight="200.0" prefWidth="200.0" />
      <VBox alignment="CENTER" fillWidth="false" spacing="5.0">
         <children>
						<ImageView fx:id="titleImage" fitHeight="100.0" fitWidth="400.0" pickOnBounds="true" preserveRatio="true">
							<image>
								<Image url="@../resources/flappyLog.png" />
							</image>
						</ImageView>
      				<VBox fx:id="buttonsPane" alignment="CENTER">
      					<children>
      						<Button fx:id="onePlayerButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onePlayerButtonAction" text="One Player">
      							<VBox.margin>
      								<Insets bottom="10.0" top="10.0" />
      							</VBox.margin>
      						</Button>
      						<Button fx:id="twoPlayerButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#twoPlayerButtonAction" text="Two Player">
      							<VBox.margin>
      								<Insets bottom="10.0" />
      							</VBox.margin>
      						</Button>
      						<Button fx:id="highScoreButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#highScoreButtonAction" text="HighScores">
      							<VBox.margin>
      								<Insets bottom="10.0" />
      							</VBox.margin>
      						</Button>
      						<Button fx:id="optionsButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#optionsButtonAction" text="Options">
      							<VBox.margin>
      								<Insets bottom="10.0" />
      							</VBox.margin>
      						</Button>
      						<Button fx:id="aboutButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#aboutButtonAction" text="About">
      							<VBox.margin>
      								<Insets bottom="10.0" />
      							</VBox.margin>
      						</Button>
      						<Button fx:id="exitButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#exitButtonAction" text="Exit" />
      					</children>
      					<opaqueInsets>
      						<Insets />
      					</opaqueInsets>
      				</VBox>
         </children>
      </VBox>
	</children>
</fx:root>
